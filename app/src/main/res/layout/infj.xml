<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff">

    <TextView
        android:id="@+id/textViewType"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="180dp"
        android:layout_marginTop="398dp"
        android:layout_marginEnd="180dp"
        android:layout_marginBottom="399dp"
        android:fontFamily="@font/noto_sans_cj_kkr_medium"
        android:lineSpacingExtra="9sp"
        android:text="INFJ"
        android:textColor="#3c3c3c"
        android:textSize="20sp"
        android:textStyle="normal" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="71dp"
        android:layout_marginTop="457dp"
        android:layout_marginEnd="70dp"
        android:fontFamily="@font/noto_sans_cj_kkr"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="14sp"
        android:text="당신은 테레사 수녀와 같은"
        android:textColor="#3c3c3c"
        android:textSize="24sp"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="72dp"
        android:layout_marginTop="506dp"
        android:layout_marginEnd="69dp"
        android:fontFamily="@font/noto_sans_cj_kkr"
        android:gravity="center_horizontal"
        android:lineSpacingExtra="14sp"
        android:text="너그러운 품성의 소유자!"
        android:textColor="#1c7eff"
        android:textSize="24sp"
        android:textStyle="bold" />

    <android.widget.Button
        android:id="@+id/button_login"
        android:layout_width="344dp"
        android:layout_height="60dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="42dp"
        android:layout_marginLeft="42dp"
        android:layout_marginTop="579dp"
        android:layout_marginEnd="42dp"
        android:layout_marginBottom="92dp"
        android:background="@drawable/btn_round_blue_box"
        android:text="확인"
        android:textColor="#ffffff"
        android:textSize="18sp" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="360dp"
        android:layout_height="422dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="27dp"
        android:layout_marginTop="0dp"
        android:layout_marginEnd="24dp"
        app:srcCompat="@drawable/jj_infj" />

</RelativeLayout>